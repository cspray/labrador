{
    "name": "cspray/labrador",
    "description": "A microframework wiring together high-quality libraries to route an HTTP request to a specific controller",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sprayberry",
            "email": "cspray+labrador@gmail.com",
            "homepage": "http://labrador.cspray.net",
            "role": "Benevolent Dictator For Life"
        }
    ],
    "require": {
        "php": "5.4.*",
        "nikic/fast-route": "0.1.0",
        "symfony/http-kernel": "2.4.*",
        "rdlowrey/auryn": "0.13.0",
        "cspray/configlet": "0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "bin": ["bin/labrador-skeleton.sh"],
    "autoload": {
        "psr-0": {
            "Labrador\\": ["src/", "test/"],
            "LabradorDemo\\": ["src/"]
        }
    }
}
