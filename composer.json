{
    "name": "cspray/labrador",
    "description": "A microframework providing a plugin and event system to build PHP powered applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sprayberry",
            "email": "cspray+labrador@gmail.com",
            "homepage": "http://labrador.cspray.net",
            "role": "Benevolent Dictator For Life"
        }
    ],
    "require": {
        "php": "~7.0",
        "rdlowrey/auryn": "~1.4",
        "league/event": "~2.1",
        "morrisonlevi/ardent": "~0.20",
        "filp/whoops": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.12"
    },
    "autoload": {
        "psr-4": {
            "Cspray\\Labrador\\": ["src/"]
        },
        "files": ["src/_functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Cspray\\Labrador\\Test\\": ["test/"]
        }
    }
}
