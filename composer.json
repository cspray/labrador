  "require": {
    "php": ">=7.2",
    "amphp/amp": "~2.1",
    "cspray/labrador-async-event": "2.0.0-rc1",
    "rdlowrey/auryn": "~1.4"
  },
  "require-dev": {
    "amphp/phpunit-util": "dev-master",
    "cspray/labrador-coding-standard": "0.1.0",
    "phpunit/phpunit": "~7"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\Labrador\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
