{
    "name": "cspray/labrador",
    "description": "A microframework providing a plugin and event system to build PHP powered applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sprayberry",
            "email": "cspray+labrador@gmail.com",
            "homepage": "http://labrador.cspray.net",
            "role": "Benevolent Dictator For Life"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cspray/phpunit-mock-objects"
        }
    ],
    "require": {
        "php": "~7.0",
        "rdlowrey/auryn": "~1.1",
        "league/event": "~2.1",
        "morrisonlevi/ardent": "~0.14",
        "filp/whoops": "~2.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1.3",
        "cspray/telluris": "~0.1"
    },
    "suggest": {
        "cspray/telluris": "For managing your environment configuration."
    },
    "autoload": {
        "psr-4": {
            "Cspray\\Labrador\\": ["src/"]
        },
        "files": ["src/_functions.php"]
    },
    "autoload-dev": {
        "psr-0": {
            "Cspray\\Labrador\\Test\\": ["test/"]
        }
    }
}
