{
    "name": "cspray/labrador",
    "description": "A microframework providing a plugin and event system to build PHP powered applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Sprayberry",
            "email": "cspray+labrador@gmail.com",
            "homepage": "http://labrador.cspray.net",
            "role": "Benevolent Dictator For Life"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cspray/Ardent"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cspray/phpunit"
        },
        {
            "type": "vcs",
            "url": "https://github.com/cspray/phpunit-mock-objects"
        }
    ],
    "require": {
        "php": "~7.0",
        "rdlowrey/auryn": "0.15.*",
        "evenement/evenement": "2.0.*",
        "morrisonlevi/ardent": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master",
        "phpunit/phpunit-mock-objects": "dev-master"
    },
    "suggest": {
        "filp/whoops": "Library to help ensure your error output doesn't suck."
    },
    "autoload": {
        "psr-0": {
            "Labrador\\": ["src/", "test/"]
        }
    }
}
